		<AssemblyAttribute Include="System.Runtime.CompilerServices.InternalsVisibleTo">
			<_Parameter1>BitzArt.Blazor.Auth.SampleApp.Server</_Parameter1>
		</AssemblyAttribute>
	</ItemGroup>

	<ItemGroup>
		<ProjectReference Include="..\..\..\src\BitzArt.Blazor.Auth\BitzArt.Blazor.Auth.csproj" />
